<h1>New gif</h1>

<%= form_for @gif do |f| %>
  <% f.object.errors.each do |att, msg| %>
    <div class="errors">
      <%= msg %>
    </div>
  <% end %>
  <%= f.label :url %>
  <%= f.text_field :url %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit "Create gif" %>
<% end %>